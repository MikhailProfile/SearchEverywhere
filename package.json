{
	"name": "ads-searcheverywhere",
	"displayName": "Search Everywhere",
	"description": "Search tables, views, stored procedures, functions by name or code.",
	"icon": "png/search.png",
	"publisher": "MikhailT",
	"version": "1.1.1",
	"engines": {
		"vscode": "^1.39.0",
		"azdata": "*"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ads-searcheverywhere.search",
		"onCommand:ads-searcheverywhere.searchAndCacheClear"
	],
	"main": "./out/searchEverywhere.js",
	"contributes": {
		"commands": [
			{
				"command": "ads-searcheverywhere.search",
				"title": "Search Everywhere"
			},
			{
				"command": "ads-searcheverywhere.searchAndCacheClear",
				"title": "Search Everywhere With Cache Clear"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Search Everywhere",
			"properties": {
				"searchEverywhere.columnsInTable": {
					"type": "boolean",
					"default": false,
					"description": "Add columns information to Tables (could have performance impact)"
				},
				"searchEverywhere.viewScriptOperation": {
					"type": "string",
					"default": "Select",
					"enum": [
						"Select",
						"Alter"
					],
					"description": "The type of script that will be opened upon selecting a view"
				},
				"searchEverywhere.tableScriptOperation": {
					"type": "string",
					"default": "Select",
					"enum": [
						"Select",
						"Create"
					],
					"description": "The type of script that will be opened upon selecting a table"
				}
			}
		},
		"keybindings": [
			{
				"key": "ctrl+t",
				"command": "ads-searcheverywhere.search"
			},
			{
				"key": "ctrl+shift+t",
				"command": "ads-searcheverywhere.searchAndCacheClear"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MikhailProfile/SearchEverywhere.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"proposedapi": "node installTypings.js"
	},
	"devDependencies": {
		"@types/azdata": "*",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.39.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"azdata-test": "^1.x",
		"eslint": "^6.8.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.0",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"memory-cache": "^0.2.0"
	},
	"__metadata": {
		"id": "101",
		"publisherDisplayName": "MikhailT",
		"publisherId": "MikhailT",
		"isPreReleaseVersion": false
	}
}